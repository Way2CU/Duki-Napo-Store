<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../units/schema/template.dtd">
<document>
	<cms:var name="_DOCTYPE"/>
	<html lang="$language" cms:eval="lang">
		<head>
			<title><cms:text constant="site_title"/><cms:raw text=" - "/><cms:text constant="menu_vip"/></title>
			<cms:module
				name="shop"
				action="include_cart_scripts"
				/>
			<cms:collection include="page_control,animation_chain"/>
			<cms:module name="head_tag" action="print_tag"/>
		</head>

		<body>
			<cms:template file="parts/header.xml"/>
			<section id="vip">
				<div class="contact_vip">
					<p><cms:text constant="title_vip"/></p>
					<img src="_BASEURL.'/site/images/title.svg'" alt="dukivenapo memebers club" cms:eval="src"/>
					<p><cms:text constant="subtitle_vip"/></p>
					<cms:desktop>
						<cms:module
							name="gallery"
							action="show_image_list"
							group="vip_gallery"
							template="parts/home_page_image.xml"
							/>
					</cms:desktop>

					<cms:mobile>
						<cms:module
							name="gallery"
							action="show_image_list"
							group="mobile_vip_gallery"
							template="parts/home_page_image.xml"
							/>
					</cms:mobile>

					<cms:module
						name="contact_form"
						action="show"
						text_id="contact_vip"
						/>
				</div>
			</section>

			<cms:template file="parts/footer.xml"/>
		</body>
	</html>
</document>